<%= render 'admin/shared/header' %>

<div class="page-container row">
  <%= render 'admin/shared/sidebar' %>

  <div class="page-content">
    <div class="content">
      <div class="page-title">
        <i class="fa fa-picture-o"></i>
        <h3><%= @gallery.name %></h3>
      </div>

      <div class="row">
        <div class="col-md-12">
          <div class="grid simple">
            <div class="grid-body">
              <%= render 'shared/feature_slider' %>
            </div><%# /grid-body %>
          </div><%# /grid %>
        </div><%# /col-md-12 %>
      </div><%# /row %>
    </div><%# /content %>
  </div><%# /page-content %>

</div><%# /page-container %>
